{
  "name": "blog-manager",
  "version": "1.0.0",
  "description": "Manage portfolio blog posts",
  "main": "index.js",
  "author": "Thomas Moawad",
  "license": "MIT",
  "scripts": {
    "sync_blog": "aws s3 sync blog s3://my-cdn-s3/portfolio/blog",
    "invalidate_cdn": "aws cloudfront create-invalidation --distribution-id E100N2BSOXPXX3 --paths '/portfolio/blog/*'",
    "create_metadata": "ts-node scripts/createMetaData.ts",
    "go": "yarn create_metadata && yarn sync_blog && yarn invalidate_cdn"
  },
  "experiments": {
    "topLevelAwait": true
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "typescript": "^5.0.4"
  }
}
